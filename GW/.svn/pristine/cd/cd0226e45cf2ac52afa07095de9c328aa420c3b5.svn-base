- (CGRect)placeholderRectForBounds:(CGRect)bounds
{
    int margin =5;
    CGRect inset =CGRectMake(bounds.origin.x + margin, bounds.origin.y+2, bounds.size.width - margin, bounds.size.height);
    return inset;
}

- (CGRect)editingRectForBounds:(CGRect)bounds
{
    int margin =5;
    CGRect inset =CGRectMake(bounds.origin.x + margin, bounds.origin.y+2, bounds.size.width - margin, bounds.size.height);
    return inset;
}


@end
